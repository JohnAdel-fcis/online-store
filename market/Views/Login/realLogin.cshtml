@model string
@{
    ViewBag.Title = "RealLogin";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2 class="text-center">Sign in </h2>
    <div class="d-flex justify-content-center">
        
        <p class="alert-danger"> @Model</p>
        <form method="post">
            <div class="form-group">
                <label>email</label>
                <input class="form-control" type="text" name="emailFromUser" />
            </div>
            <div class="form-group">
                <label>Password</label>
                <input class="form-control" type="password" name="passwordFromUser" />
            </div>
            <button type="submit" class="btn btn-primary" style="width: 280px"> Login </button>
            <p style="margin: 30px"> don't have account !! <a href="~/Login/Index">Register</a></p>
        </form>
        
    </div>

